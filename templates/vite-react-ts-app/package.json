{
  "name": "vite-react-ts-app",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "serve": "vite preview",
    "format": "prettier --check .",
    "lint": "eslint .",
    "types": "tsc --noEmit",
    "test": "jest",
    "test:cypress": "cypress open",
    "precommit": "yarn format && yarn lint && yarn types"
  },
  "dependencies": {
    "react": "17.0.2",
    "react-dom": "17.0.2"
  },
  "devDependencies": {
    "@matejbransky/eslint-config": "1.0.0-alpha.8",
    "@matejbransky/jest-preset": "1.0.0-alpha.3",
    "@matejbransky/prettier-config": "1.0.0-alpha.2",
    "@testing-library/cypress": "7.0.6",
    "@testing-library/jest-dom": "5.11.10",
    "@testing-library/react": "11.2.6",
    "@types/jest": "26.0.24",
    "@types/react": "17.0.3",
    "@types/react-dom": "17.0.3",
    "@vitejs/plugin-react-refresh": "1.3.2",
    "cypress": "7.0.1",
    "eslint": "7.23.0",
    "eslint-plugin-jest": "24.3.4",
    "jest": "26.6.3",
    "prettier": "2.2.1",
    "typescript": "4.2.4",
    "vite": "2.2.1"
  },
  "jest": {
    "preset": "@matejbransky/jest-preset",
    "testEnvironment": "jsdom",
    "modulePathIgnorePatterns": [
      "<rootDir>/cypress/"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/jest-setup.ts"
    ]
  },
  "prettier": "@matejbransky/prettier-config",
  "eslintConfig": {
    "extends": "@matejbransky/eslint-config",
    "plugins": [
      "jest"
    ],
    "env": {
      "jest/globals": true
    }
  }
}
